<h1>Listing genes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Source</th>
      <th>Desctiption</th>
      <th>Uniprotkb</th>
      <th>Ensemblid</th>
      <th>Entrezid</th>
      <th>Pharmgkbid</th>
      <th>Drugbanktargetid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @genes.each do |gene| %>
      <tr>
        <td><%= gene.name %></td>
        <td><%= gene.source %></td>
        <td><%= gene.desctiption %></td>
        <td><%= gene.uniprotKB %></td>
        <td><%= gene.ensemblID %></td>
        <td><%= gene.entrezID %></td>
        <td><%= gene.pharmGkbID %></td>
        <td><%= gene.drugbankTargetID %></td>
        <td><%= link_to 'Show', gene %></td>
        <td><%= link_to 'Edit', edit_gene_path(gene) %></td>
        <td><%= link_to 'Destroy', gene, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gene', new_gene_path %>
